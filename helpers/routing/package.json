{
  "name": "meiosis-routing",
  "version": "1.3.0",
  "private": true,
  "description": "Meiosis Helpers - Routing",
  "main": "meiosis-routing.js",
  "types": "index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "watch": "npm run check -- --watch",
    "lib": "tsc",
    "build": "rollup -c && cp -r src/* dist/",
    "start": "npm run lib && npm run build && cat package.json|grep -v private>dist/package.json",
    "test": "jest --config jestconfig.json",
    "docs": "typedoc --excludeNotExported --out docs/ src",
    "onchange": "onchange -i index.ts 'state/*' 'router-helper/*' -- npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis.git"
  },
  "keywords": [
    "meiosis",
    "reactive",
    "routing",
    "state management"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "feather-route-matcher": "^3.1.0",
    "jest": "^24.8.0",
    "onchange": "^6.0.0",
    "prettier": "1.18.2",
    "query-string": "^6.8.1",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3",
    "url-mapper": "^2.0.0"
  }
}
