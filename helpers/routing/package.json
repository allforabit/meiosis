{
  "name": "meiosis-routing",
  "version": "1.0.0",
  "description": "Meiosis Helpers - routing",
  "main": "dist/meiosis-routing.js",
  "scripts": {
    "check": "tsc --noEmit",
    "watch": "npm run check -- --watch",
    "lib": "tsc --emitDeclarationOnly",
    "build": "rollup -c",
    "start": "npm run lib && npm run build",
    "test": "npm run build && tape 'tests/**/*.js' | tap-spec",
    "docs": "typedoc --excludeNotExported --out docs/ index.ts state router-helper"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis.git"
  },
  "keywords": [
    "meiosis",
    "reactive",
    "routing",
    "state management"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "feather-route-matcher": "^3.0.0",
    "prettier": "1.17.1",
    "query-string": "^6.5.0",
    "rollup": "^1.15.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1",
    "url-mapper": "^2.0.0"
  }
}
